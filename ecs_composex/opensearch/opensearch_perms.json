    "RO": {
      "Effect": "Allow",
      "Action": [
        "es:DeleteDomain",
        "es:DescribeDomain",
        "es:DescribeDomainAutoTunes",
        "es:DescribeDomainConfig",
        "es:ESCrossClusterGet",
        "es:GetCompatibleVersions",
        "es:GetUpgradeHistory",
