{
    "Consumer": {
        "Effect": "Allow",
        "Action": [
            "kinesis:Get*",
            "kinesis:DescribeStreamSummary"
        ]
    },
    "Producer": {
        "Effect": "Allow",
        "Action": [
            "kinesis:PutRecord"
        ]
    },
    "PowerUser": {
        "Effect": "Allow",
        "NotAction": [
            "kinesis:CreateStream",
            "kinesis:DeleteStream"
        ]
    }
}
